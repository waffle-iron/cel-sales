            </column>
            
            <column name="alm_interno_disp" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="alm_externo_total" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="alm_externo_disp" type="varchar(255)">
